{
	"name": "elvebakkenrevyen",
	"version": "0.0.2",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"start": "vite preview --host 0.0.0.0 --port 8080",
		"build:css": "postcss src/app.css -o dist/styles.css",
		"build": "NODE_ENV=production npm run build:css && vite build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.49",
		"postcss-cli": "^11.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"purgecss": "^7.0.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.7.1",
		"@fortawesome/svelte-fontawesome": "^0.2.3",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"flowbite-svelte": "^0.47.3",
		"lodash.clonedeep": "^4.5.0",
		"lodash.get": "^4.4.2",
		"lodash.isequal": "^4.5.0",
		"postcss-purgecss": "^5.0.0"
	}
}
